<idea-plugin>
    <id>com.alekseyzhelo.evilislands.mobplugin</id>
    <name>Evil Islands Mob-file Support</name>
    <version>1.0</version>
    <vendor email="alekseyzhelo@yandex.ru" url="http://gipatgroup.org">GipatGroup</vendor>

    <description><![CDATA[
      Enter short description for your plugin here.<br>
      <em>most HTML tags may be used</em>
    ]]></description>

    <change-notes><![CDATA[
      Add change notes here.<br>
      <em>most HTML tags may be used</em>
    ]]>
    </change-notes>

    <!-- please see https://confluence.jetbrains.com/display/IDEADEV/Build+Number+Ranges for description -->
    <idea-version since-build="131"/>

    <!-- please see https://confluence.jetbrains.com/display/IDEADEV/Plugin+Compatibility+with+IntelliJ+Platform+Products
         on how to target different products -->
    <!-- uncomment to enable plugin in all products
    <depends>com.intellij.modules.lang</depends>
    -->

    <extensions defaultExtensionNs="com.intellij">
        <fileTypeFactory implementation="com.alekseyzhelo.evilislands.mobplugin.script.fileType.ScriptFileTypeFactory"/>
        <lang.parserDefinition language="EIScriptLanguage" implementationClass="com.alekseyzhelo.evilislands.mobplugin.script.EIScriptParserDefinition"/>
        <lang.syntaxHighlighterFactory key="EIScriptLanguage" implementationClass="com.alekseyzhelo.evilislands.mobplugin.script.highlighting.EIScriptSyntaxHighlighterFactory"/>
        <colorSettingsPage implementation="com.alekseyzhelo.evilislands.mobplugin.script.highlighting.EIScriptColorSettingsPage"/>
        <lang.refactoringSupport language="EIScriptLanguage" implementationClass="com.alekseyzhelo.evilislands.mobplugin.script.codeInsight.EIScriptRefactoringSupportProvider"/>
        <lang.elementManipulator forClass="com.alekseyzhelo.evilislands.mobplugin.script.psi.EIScriptIdentifier"
                                 implementationClass="com.alekseyzhelo.evilislands.mobplugin.script.psi.manipulators.ScriptIdentifierManipulator"/>
        <lang.findUsagesProvider language="EIScriptLanguage" implementationClass="com.alekseyzhelo.evilislands.mobplugin.script.SimpleScriptFindUsagesProvider"/>
        <lang.foldingBuilder language="EIScriptLanguage" implementationClass="com.alekseyzhelo.evilislands.mobplugin.script.EIScriptSimpleFoldingBuilder"/>
        <annotator language="EIScriptLanguage" implementationClass="com.alekseyzhelo.evilislands.mobplugin.script.highlighting.EIScriptHighlightingAnnotator"/>
        <annotator language="EIScriptLanguage" implementationClass="com.alekseyzhelo.evilislands.mobplugin.script.codeInsight.EIScriptAnnotator"/>
        <lang.documentationProvider language="EIScriptLanguage" implementationClass="com.alekseyzhelo.evilislands.mobplugin.script.codeInsight.EIScriptDocumentationProvider"/>
        <completion.contributor language="EIScriptLanguage" implementationClass="com.alekseyzhelo.evilislands.mobplugin.script.codeInsight.EIScriptCompletionContributor"/>
        <lang.psiStructureViewFactory language="EIScriptLanguage"
                                      implementationClass="com.alekseyzhelo.evilislands.mobplugin.script.structureView.EIScriptStructureViewBuilderFactory"/>

        <virtualFileSystem key="mob"
                           implementationClass="com.alekseyzhelo.evilislands.mobplugin.mob.vfs.MobAsArchiveFileSystem"/>
        <fileType name="mob"
                  patterns="*.mob"
                  implementationClass="com.alekseyzhelo.evilislands.mobplugin.mob.fileType.TestMobFileType"/>
        <treeStructureProvider
                implementation="com.alekseyzhelo.evilislands.mobplugin.projectView.EIMobStructureProvider"/>
    </extensions>

    <application-components>
        <!-- Add your application components here -->
    </application-components>

    <project-components>
        <!-- Add your project components here -->
    </project-components>

    <actions>
        <!-- Add your actions here -->
    </actions>

</idea-plugin>